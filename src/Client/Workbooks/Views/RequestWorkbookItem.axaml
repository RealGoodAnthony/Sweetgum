<reactiveUi:ReactiveUserControl x:TypeArguments="viewModels:RequestWorkbookItemViewModel" xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:viewModels="clr-namespace:WillowTree.Sweetgum.Client.Workbooks.ViewModels"
                    xmlns:reactiveUi="http://reactiveui.net"
                    mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                    x:Class="WillowTree.Sweetgum.Client.Workbooks.Views.RequestWorkbookItem">
    <UserControl.Styles>
        <Style Selector="StackPanel#RowStackPanel">
            <Setter Property="Background" Value="Transparent" />
        </Style>
        <Style Selector="StackPanel#RowStackPanel:pointerover">
            <Setter Property="Background" Value="#48DACB" />
        </Style>
    </UserControl.Styles>
    <UserControl x:Name="RowUserControl" Cursor="Hand">
        <StackPanel Orientation="Vertical">
            <TextBlock x:Name="NameTextBlock" Foreground="#223547" />
        </StackPanel>
    </UserControl>
</reactiveUi:ReactiveUserControl>
